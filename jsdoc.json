{
  "source": {
    "include": ["./lib"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md",
    "package": "./package.json"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm"
  }
}